{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ],
    "rules": {
      "no-console": false
    }
  },
  "rules": {
    "no-console": false,
    "ordered-imports": false
  },
  "compilerOptions": {
    "typeRoots": [
      "node_modules/@types"
    ]
  }
}